<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.example"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>TechBubble</name>

    <description>
        A simple demonstration app build on PhoneGap, for 3months.com
    </description>

    <author href="https://joshmcarthur.com" email="josh@3months.com">
        Josh McArthur
    </author>

    <preference name="orientation" value="portrait" />

    <!-- Icons -->
    <icon src="icon.png" width="57" height="57" />
    <icon src="icon.png" gap:platform="ios" width="72" height="72" />
    <icon src="icon.png" width="114" height="114" />

    <icon src="icon.png" gap:platform="android" gap:density="ldpi" />
    <icon src="icon.png" gap:platform="android" gap:density="mdpi" />
    <icon src="icon.png" gap:platform="android" gap:density="hdpi" />
    <icon src="icon.png" gap:platform="android" gap:density="xhdpi" />

</widget>